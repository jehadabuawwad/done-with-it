{
  "expo": {
    "name": "Done-With-It",
    "slug": "Done-With-It",
    "version": "1.0.0",
    "orientation": "protrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": "Done With it is a react native mobile application as a market place for selling the stuff you don't need.\n\nIt's a place you can put things you don't need and sell it for other user whom using this application.",
    "githubUrl": "https://github.com/Jehadabuawwad/done-with-it.git"
  }
}
